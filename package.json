{
  "name": "@fartts/fartts",
  "version": "1.24.0",
  "description": "ùëì(unctional)Art(-making).·¥õ(ype)Íú±(cript) - an experiment by [Matt Hayes](https://github.com/mysterycommand)",
  "repository": "https://github.com/fartts/fartts.git",
  "author": "Matt Hayes <matt@mysterycommand.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "update": "rustup update && cargo install-update -a",
    "reset": "yarn clean node_modules/ && yarn install",
    "clean": "cargo clean && rm -rf .cache/ coverage/ dist/",
    "build": "parcel build src/lab/{index.html,README.md} --public-url /",
    "serve": "yarn build && npx serve dist",
    "start": "parcel src/lab/{index.html,README.md}",
    "lint": "yarn lint:rs && yarn lint:ts",
    "lint:rs": "cargo fmt --all -- --check && cargo clippy",
    "lint:ts": "tslint 'src/**/*.ts'",
    "test": "cargo test && jest",
    "coverage": "open coverage/lcov-report/index.html",
    "lab": "node ./src/cli"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.11.2",
    "yarn": "1.19.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "core-js": "3.3.3",
    "seedrandom": "3.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@iarna/toml": "2.2.3",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.5.4",
    "@semantic-release/npm": "5.2.0",
    "@semantic-release/release-notes-generator": "7.3.0",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/seedrandom": "2.4.28",
    "babel-jest": "24.9.0",
    "cheerio": "1.0.0-rc.3",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "glslify-bundle": "5.1.1",
    "glslify-deps": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-wasm-pack": "4.1.2",
    "prettier": "1.18.2",
    "semantic-release": "15.13.27",

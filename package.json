{
  "name": "@fartts/fartts",
  "version": "1.29.1",
  "description": "ùëì(unctional)Art(-making).·¥õ(ype)Íú±(cript) - an experiment by [Matt Hayes](https://github.com/mysterycommand)",
  "repository": "https://github.com/fartts/fartts.git",
  "author": "Matt Hayes <matt@mysterycommand.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "update": "rustup update && cargo install-update -a",
    "reset": "yarn clean node_modules/ && yarn install && yarn update",
    "clean": "cargo clean && rm -rf .cache/ coverage/ dist/",
    "build": "parcel build src/lab/{index.html,README.md} --public-url /",
    "serve": "yarn build && npx serve dist",
    "start": "parcel src/lab/{index.html,README.md}",
    "lint": "yarn lint:rs && yarn lint:ts",
    "lint:rs": "cargo fmt --all -- --check && cargo clippy",
    "lint:ts": "tslint 'src/**/*.ts'",
    "test": "cargo test && jest",
    "coverage": "open coverage/lcov-report/index.html",
    "lab": "node ./src/cli",
    "screenshots": "ts-node --project scripts/tsconfig.json scripts/screenshots.ts"
  },
  "engines": {
    "node": ">=12",
    "npm": "6.14.8",
    "yarn": "1.22.10"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "core-js": "3.6.5",
    "seedrandom": "3.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@iarna/toml": "2.2.5",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.1.1",
    "@semantic-release/npm": "7.0.6",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/dom-mediacapture-record": "1.0.7",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/puppeteer": "3.0.4",
    "@types/seedrandom": "2.4.28",
    "babel-jest": "26.6.1",
    "cheerio": "1.0.0-rc.3",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "glslify-bundle": "5.1.1",
    "glslify-deps": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "lodash": "4.17.20",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-wasm-pack": "6.0.1",
    "prettier": "2.1.2",
    "puppeteer": "5.4.1",
    "semantic-release": "17.2.2",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5"
  }
}
